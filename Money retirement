    int c;
}
int main()

{
    int B,b,c,A,a;
    cin >> B >> b >> c>> A >> a;
    
    if ( B <  101 && B > 19)
    {
        if ( b < B)
        {
            return 0;
        }
        
    }
    do
    {
        for (int i = b ; i <= B ; b--)
             {
                 
               S(c);
               --b;
             }
    }
    while (B==b);
    
    
    
    if (A > 101 || A < 19)
    {
        return 0;
    }
    
    do
    {
        cout << S << endl;
        ++A;
    }
    while ( a < c);
    
    cout << A << endl;
    
    return 0;
        
    }
